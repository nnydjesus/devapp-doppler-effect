<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="projectRepo" class="ar.edu.unq.dopplereffect.persistence.repositories.ProjectRepositoryImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	
	<bean id="skillRepo" class="ar.edu.unq.dopplereffect.persistence.repositories.SkillRepositoryImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="careerDataRepo" class="ar.edu.unq.dopplereffect.persistence.repositories.CareerDataRepositoryImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="employeeRepo" class="ar.edu.unq.dopplereffect.persistence.repositories.EmployeeRepositoryImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	
	<bean id="careerPlanLevelRepo" class="ar.edu.unq.dopplereffect.persistence.repositories.CareerPlanLevelRepositoryImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	
	<bean id="salarySpecRepo" class="ar.edu.unq.dopplereffect.persistence.repositories.SalarySpecificationRepositoryImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	
	<bean id="leaveReqRepo" class="ar.edu.unq.dopplereffect.persistence.repositories.LeaveRequestRepositoryImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	
	<bean id="leaveReqTypeRepo" class="ar.edu.unq.dopplereffect.persistence.repositories.LeaveRequestTypeRepositoryImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	
	<bean id="userRepo" class="ar.edu.unq.dopplereffect.persistence.repositories.UserRepositoryImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
</beans>