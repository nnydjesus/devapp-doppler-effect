<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="userService" class="ar.edu.unq.dopplereffect.service.LoginService" >
		<property name="repository" ref="userRepo"/>
	</bean>
	
	<bean id="projectService" class="ar.edu.unq.dopplereffect.service.ServiceImpl">
		<property name="repository" ref="projectRepo" />
	</bean>
	
	<bean id="employeeService" class="ar.edu.unq.dopplereffect.service.ServiceImpl">
		<property name="repository" ref="employeeRepo" />
	</bean>
	
	<bean id="salarySpecService" class="ar.edu.unq.dopplereffect.service.ServiceImpl">
		<property name="repository" ref="salarySpecRepo" />
	</bean>
	
	<bean id="leaveReqService" class="ar.edu.unq.dopplereffect.service.LeaveRequestServiceImpl">
		<property name="repository" ref="leaveReqRepo" />
	</bean>
	
	<bean id="leaveReqTypeService" class="ar.edu.unq.dopplereffect.service.ServiceImpl">
		<property name="repository" ref="leaveReqTypeRepo" />
	</bean>
	
	<bean id="skillService" class="ar.edu.unq.dopplereffect.service.ServiceImpl">
		<property name="repository" ref="skillRepo" />
	</bean>
	
	<bean id="careerPlanLevelService" class="ar.edu.unq.dopplereffect.service.ServiceImpl">
		<property name="repository" ref="careerPlanLevelRepo" />
	</bean>
	
	<bean id="addDefaultValuesService" class="ar.edu.unq.dopplereffect.service.AddDefaultValuesService">
		<property name="careerPlanLevelRepo" ref="careerPlanLevelRepo" />
		<property name="employeeRepo" ref="employeeRepo" />
		<property name="projectRepo" ref="projectRepo" />
		<property name="leaveReqRepo" ref="leaveReqRepo" />
		<property name="leaveReqTypeRepo" ref="leaveReqTypeRepo" />
		<property name="salarySpecRepo" ref="salarySpecRepo" />
	</bean>
	
</beans>